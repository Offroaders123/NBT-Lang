{
  "name": "nbt-lang",
  "displayName": "NBT Lang",
  "description": "An LSP service for NBT and SNBT files!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nbt",
        "aliases": ["NBT", "nbt"],
        "extensions": [".nbt", ".dat", ".dat_old", ".snbt"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nbt",
        "scopeName": "source.nbt",
        "path": "./syntaxes/nbt.tmLanguage.json"
      }
    ]
  }
}